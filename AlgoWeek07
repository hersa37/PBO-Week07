KantorCabang
attribute:
	string kodeCabang
	Pegawai pimpinan
	Pegawai[] pegawaiArray
	int jumlahPegawai

constructor()
	this("",new Pegawai("-","-","Pimpinan"), new Pegawai[0])

constructor(string kode, Pegawai pimpinan)
	kode_cabang=kode
	if(pimpinan.getJabatan=="pimpinan")
		this.pimpinan=pimpinan
	else this.pimpinan=new Pegawai("-","-","pimpinan")
	
setKodeCabang(string kode)
getKodeCabang()
setPimpinan(Pegawai pimpinan)
	if(pimpinan.getJabatan==pimpinan)
		this.pimpinan=pimpinan
getPimpinan()
getJumlahPegawai()
	return pegawaiArray.length
setPegawaiArray(Pegawai[] pegawaiArray,int jumlahPegawai)
getPegawaiArray()
toString()


mainKancab
constant:
	int maxPegawai=100;
main method
	int jumlahPegawai
	Pegawai[] pegawaiArray=new Pegawai[maxPegawai]
	
	KantorCabang kc=new KantorCabang("B001", new Pegawai(), pegawaiArray, jumlahPegawai)
	for(i=0; i<jumlahPegawai;i++)
		pegawaiArray[i]=new Pegawai()
		
